    <connection>OpenMRS</connection>
    <sql>&#x2f;&#x2a;&#xa;  Extracts all patient identifiers&#xa; &#x2a;&#x2f;&#xa;select      i.patient_identifier_id,&#xa;			i.uuid as uuid,&#xa;			p.patient_id,&#xa;            i.identifier_type,&#xa;            i.identifier,&#xa;            i.location_id&#xa;from        patient p&#xa;left join   patient_identifier i on i.patient_id &#x3d; p.patient_id&#xa;where       i.voided &#x3d; 0 and p.voided &#x3d; 0&#xa;and			i.identifier is not null&#xa;and 		i.identifier &#x21;&#x3d; &#x27;&#x27;</sql>
    <limit>0</limit>
