    <name>Import Encounter</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>OpenMRS</connection>
    <sql>&#x2f;&#x2a;&#xa;	Extracts all encounteres&#xa;&#x2a;&#x2f;&#xa;select		e.uuid,&#xa;			e.location_id,&#xa;			e.encounter_type as encounter_type_id,&#xa;			e.encounter_datetime,&#xa;			e.patient_id,&#xa;			p.uuid as patient_uuid,&#xa;			TIMESTAMPDIFF&#x28;YEAR, p.birthdate, e.encounter_datetime&#x29; as age_at_enc&#xa;from 		encounter e&#xa;left join 	person p on e.patient_id &#x3d; p.person_id&#xa;where		e.voided &#x3d; 0&#xa;order by	e.uuid</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
