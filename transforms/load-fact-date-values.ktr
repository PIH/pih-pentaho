    <sql>SELECT      p.patient_id, g.gender_id, a.age_id, d.date_id, t.time_id, l.location_id, e.encounter_type_id, q.date_question_id, v.date_id as date_value_id&#xa;FROM        omrs_obs o&#xa;INNER JOIN  dim_patient p on o.patient_id &#x3d; p.patient_id&#xa;INNER JOIN  dim_date_question q on q.name &#x3d; o.concept&#xa;INNER JOIN  dim_date v on v.full_date &#x3d; o.value_date&#xa;LEFT JOIN 	dim_gender g on g.code &#x3d; p.gender&#xa;LEFT JOIN   dim_age a on a.full_years &#x3d; o.age_years_at_obs and a.full_months &#x3d; o.age_months_at_obs&#xa;LEFT JOIN   dim_date d on d.full_date &#x3d; o.obs_date&#xa;LEFT JOIN   dim_time t on t.full_time &#x3d; o.obs_time&#xa;LEFT JOIN   dim_location l on l.name &#x3d; o.location&#xa;LEFT JOIN   dim_encounter_type e on e.name &#x3d; o.encounter_type&#xa;WHERE       o.value_date is not NULL</sql>
    <limit>0</limit>
