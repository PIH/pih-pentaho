  <step>
    <name>import program stats</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>OpenMRS</connection>
    <sql>/*
 import patient's program statistics
*/

SELECT p.name, p.concept_id,
SUM(CASE WHEN pp.date_enrolled IS NOT NULL THEN 1 else 0 END) as `ever_enrolled`,
sum(case when cn.name='Patient completed treatment' then 1 else 0 end) as `completed_treatment`,
sum(case when cn.name='Patient transferred out' then 1 else 0 end) as `transferred_out`,
sum(case when cn.name='Patient died' then 1 else 0 end) as `dead`,
sum(case when cn.name='Lost to followup' then 1 else 0 end) as `lost_to_followup`,
sum(case when cn.name='Patient refused treatment' then 1 else 0 end) as `patient_refused_treatment`
from program p left join patient_program pp using (program_id)
left join concept_name cn on pp.outcome_concept_id=cn.concept_id group by p.program_id;</sql>
