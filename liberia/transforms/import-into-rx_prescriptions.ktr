    <connection>OpenMRS</connection>
    <sql>/*
 importing prescriptions
*/

SELECT 
d.obs_group_id,
e.encounter_id,
e.patient_id,
et.encounter_type_id,
et.name as encounter_type,
MAX(CASE when d.d_concept_name = "Mental health medication" or d.d_concept_name = "Medication orders" then d.d_vcoded_concept_name end) as rx_medication,
MAX(CASE when d.d_concept_name = "Quantity of medication prescribed per dose" then d.d_value_numeric end) as rx_prescription_quantity,
MAX(CASE when d.d_concept_name = "Dosing units, coded" then d.d_vcoded_concept_name end) as rx_dose_unit,
MAX(CASE when d.d_concept_name = "HUM Drug frequency" then d.d_vcoded_concept_name end) as rx_drug_frequency,
MAX(CASE when d.d_concept_name = "Medication duration" then d.d_value_numeric end) as rx_medication_duration,
MAX(CASE when d.d_concept_name = "Time units" then d.d_vcoded_concept_name end) as rx_time_units
FROM obs o join encounter e using (encounter_id) 
JOIN concept_name cn using (concept_id)
join encounter_type et on e.encounter_type=et.encounter_type_id 
LEFT OUTER JOIN (
	SELECT o.obs_id as d_obs_id, o.obs_group_id, c.name as d_concept_name, c2.name as d_vcoded_concept_name,
	o.value_coded as d_value_coded, o.value_datetime as d_value_datetime, o.value_numeric as d_value_numeric, o.value_text as d_value_text 
    FROM obs o JOIN concept_name c on o.concept_id=c.concept_id and c.locale = 'en' and c.locale_preferred = '1'  and c.voided = 0
    LEFT JOIN concept_name c2 on o.value_coded = c2.concept_id and c2.locale = 'en' and c2.locale_preferred = '1'  and c2.voided = 0 
) d ON o.obs_id = d.obs_group_id where cn.name ='Prescription construct' GROUP BY d.obs_group_id</sql>
