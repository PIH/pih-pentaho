        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var formatted_identifier &#x3d; identifier&#x3b;&#xa;if &#x28;formatted_identifier&#x29; &#x7b;&#xa;	var newId &#x3d; &#x27;&#x27;&#x3b;&#xa;	var pad &#x3d; &#x22;0000&#x22;&#xa;	var idarr &#x3d; formatted_identifier.split&#x28;&#x27; &#x27;&#x29;&#x3b;&#xa;	for &#x28;var i&#x3d;0&#x3b; i&#x3c;idarr.length&#x3b; i&#x2b;&#x2b;&#x29; &#x7b;&#xa;		var idElement &#x3d; idarr&#x5b;i&#x5d;&#xa;		if &#x28;newId &#x21;&#x3d; &#x27;&#x27;&#x29; &#x7b;&#xa;			newId &#x2b;&#x3d; &#x27;-&#x27;&#x3b;&#xa;		&#x7d;&#xa;		var idElementAsNum &#x3d; parseInt&#x28;idElement&#x29;&#x3b;&#xa;		if &#x28;&#x21;isNaN&#x28;idElementAsNum&#x29;&#x29; &#x7b;&#xa;			idElement &#x3d; pad.substring&#x28;0, pad.length - idElement.length&#x29; &#x2b; idElement&#xa;		&#x7d;&#xa;		newId &#x2b;&#x3d; idElement&#x3b;&#xa;	&#x7d;&#xa;	formatted_identifier &#x3d; newId&#x3b;&#xa;&#x7d;&#xa;</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>formatted_identifier</name>
        <rename>eid_number</rename>
        <type>String</type>
        <length>-1</length>
        <precision>-1</precision>
