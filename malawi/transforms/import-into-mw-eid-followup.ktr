    <connection>Warehouse</connection>
    <sql>select		e.patient_id, e.encounter_date as visit_date, e.location, o.value_coded as breast_feeding

from		omrs_encounter e
inner join 	omrs_obs o on e.encounter_id = o.encounter_id
where		o.concept = 'Breast feeding'   and e.encounter_type in ('EXPOSED_CHILD_FOLLOWUP')
group by 	e.patient_id, e.encounter_date, e.location
order by 	patient_id, visit_date, location</sql>
