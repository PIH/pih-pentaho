    <sql>select		e.patient_id, e.encounter_date as visit_date, e.location, o.value_coded as ldl, o.obs_group_id
	
from		omrs_encounter e
inner join 	omrs_obs o on e.encounter_id = o.encounter_id
where		o.concept = 'Lower than Detection Limit' and e.encounter_type in ('Viral Load Screening')
group by 	e.patient_id, e.encounter_date, e.location
order by 	patient_id, visit_date, location</sql>
