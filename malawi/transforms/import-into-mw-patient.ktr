    <connection>Warehouse</connection>
    <sql>select      p.patient_id, r.related_person as chw
from        omrs_relationship r
inner join  omrs_patient p on p.patient_id = r.patient_id
where       r.relationship_id = (
    select    r.relationship_id
    from      omrs_relationship r
    where     r.patient_id = p.patient_id
    and       r.related_person_role = 'Community Health Worker'
    order by  if(r.end_date is null, 1, 0) desc, if(r.start_date is null, 0, 1) desc, r.date_created desc
    limit     1
)
order by p.patient_id;</sql>
