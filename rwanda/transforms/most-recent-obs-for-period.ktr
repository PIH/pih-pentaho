    <sql>&#x2f;&#x2a;This is the first &#x26; last obs table&#x2a;&#x2f;&#xd;&#xa;SELECT &#xd;&#xa;pt.patient_id,&#xd;&#xa;pt.concept,&#xd;&#xa;pt.end_date,&#xd;&#xa;last_obs.last_obs_date,&#xd;&#xa;last_obs.encounter_type,&#xd;&#xa;last_obs.location,&#xd;&#xa;last_obs.value_coded,&#xd;&#xa;last_obs.value_date,&#xd;&#xa;last_obs.value_numeric,&#xd;&#xa;last_obs.value_text,&#xd;&#xa;last_obs.obs_group_id&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;from &#x28;SELECT distinct omrs_obs.patient_id, concept,end_date&#xd;&#xa;	from omrs_obs, rw_selected_periods&#xd;&#xa;    WHERE concept in &#x28;SELECT concept from rw_concepts_selected&#x29; &#xd;&#xa;    &#x29; pt &#x2f;&#x2a;This is the patient-concept list as the basis&#x2a;&#x2f;&#xd;&#xa;LEFT JOIN &#x28;																			&#x2f;&#x2a; this gives info for the last encounter&#x2a;&#x2f;&#xd;&#xa;	SELECT omrs_obs.patient_id, omrs_obs.concept, omrs_obs.obs_date as last_obs_date,omrs_obs.encounter_type,&#xd;&#xa;		omrs_obs.location,omrs_obs.value_coded,omrs_obs.value_date, omrs_obs.value_numeric,omrs_obs.value_text,omrs_obs.obs_group_id&#xd;&#xa;    from &#x28;SELECT omrs_obs.patient_id, concept, max&#x28;obs_date&#x29; as last_obs_date&#xd;&#xa;			from omrs_obs, rw_selected_periods&#xd;&#xa;            WHERE concept in &#x28;SELECT concept from rw_concepts_selected&#x29;&#xd;&#xa;            and obs_date&#x3c;&#x3d;rw_selected_periods.end_date&#xd;&#xa;            Group By omrs_obs.patient_id, omrs_obs.concept&#xd;&#xa;        &#x29; as patient_concept_last        &#x2f;&#x2a;this is the most recent obs_date grouped by patient id and concept&#x2a;&#x2f;&#xd;&#xa;			inner join omrs_obs &#xd;&#xa;				on patient_concept_last.last_obs_date &#x3d; omrs_obs.obs_date&#xd;&#xa;				and omrs_obs.patient_id&#x3d; patient_concept_last.patient_id&#xd;&#xa;				and omrs_obs.concept   &#x3d; patient_concept_last.concept &#xd;&#xa;&#x29; last_obs on last_obs.patient_id&#x3d;pt.patient_id&#xd;&#xa;&#x3b;&#xd;&#xa;</sql>
    <limit>0</limit>
