    <connection>OpenMRS</connection>
    <sql>/*
*	Extracts all non-voided orders  on non-voided patients
*   as of creating this, only new and discontinue order_actions existed. It will not handle changed orders appropriately
*/
select		o.order_id,
			o.order_type_id,
			o.concept_id,
			o.orderer,
			o.instructions,
			o.date_activated as start_date, #was start_date, but now need to modify by different order types
			o.auto_expire_date,
			discontinue_orders.discontinue_date,
			discontinue_orders.discontinue_by,
			discontinue_orders.discontinue_reason,
			o.patient_id,
			o.uuid,
			discontinue_orders.discontinue_reason_non_coded,
			o.urgency,
			o.date_created,
			if (p.birthdate is null or o.date_activated is null, null, TIMESTAMPDIFF(YEAR, p.birthdate, o.date_activated)) as age_years_at_start_date,
			if (p.birthdate is null or o.date_activated is null, null, TIMESTAMPDIFF(MONTH, p.birthdate, o.date_activated)) as age_months_at_start_date,
			
			e.encounter_id,
			e.encounter_type,
			e.location_id,

			o.order_group_id,
##			oeo.order_index, #this was never used
			o.order_reason, #was indication
			drug_order.route,
			o.comment_to_fulfiller,#administration_instructions,

			order_set.name as order_set,
			drug_order.dose,
			drug_order.dose_units,
			drug_order.frequency,
			drug_order.as_needed, #prn
#			drug_order.complex, #was included but never changed
			drug_order.quantity,
#			drug_order.equivalent_daily_dose, #was included but not used
			drug.name as drug_name
			
	
from 		orders o

inner join 	person p on o.patient_id = p.person_id
left join 	encounter e on o.encounter_id = e.encounter_id
LEFT JOIN   drug_order on drug_order.order_id=o.order_id
LEFT JOIN   drug on drug.drug_id=drug_order.drug_inventory_id
LEFT JOIN   order_group on order_group.order_group_id=o.order_group_id
left join	order_set on order_set.order_set_id=order_group.order_set_id


#			o.discontinued_date,
#			o.discontinued_by,
#			o.discontinued_reason,
LEFT JOIN (SELECT date_activated as discontinue_date, order_reason as discontinue_reason, creator as discontinue_by,  order_reason_non_coded as discontinue_reason_non_coded, previous_order_id
	from orders where voided=0 and order_action="DISCONTINUE"
    ) discontinue_orders on discontinue_orders.previous_order_id=o.order_id

where		o.voided = 0 and p.voided = 0
and order_action="NEW";
</sql>
